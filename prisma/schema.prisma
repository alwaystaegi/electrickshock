// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ElecData {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  cntrKndNm String
  salekwh   Int
  useMm     String
  useYy     String
  sidoNm    String
  custHocnt Int
  cntrPwr   Int
  sigunguNm String
}
