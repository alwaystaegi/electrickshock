generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DateData {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  month Int[]
  year  Int
}

model DayStatis {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  date           String
  nowValue       Int
  /// Multiple data types found: Float: 85.7%, Int: 14.3% out of 42 sampled entries
  reservePercent Float
  supplydemand   Int
  time           String
}

model ElecData {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  cntrKndNm String
  cntrPwr   Int
  custHocnt Int
  salekwh   Int?
  sidoNm    String
  sigunguNm String
  useMm     String
  useYy     String
}

model ListData {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  cntrKndNmList String[]
}

model SidoData {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  sidoNm        String?
  sigunguNmList String[]
}
